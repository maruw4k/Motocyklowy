<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" />
    @System.Web.Optimization.Styles.Render("~/Content/themes/base/css")
</head>
<body>
    
        <header class="page-header">
            
                <a id="indexHeaderLogo" href="@Url.Content("~/")">
                    <h1 class="logo">Komis motorowy</h1>
                </a>
                    <a class="menu-item" href="@Url.Action("StronyStatyczne", "Home", new { nazwa = "Onas" })" style="">O nas</a>
                    <a class="menu-item" href="@Url.Action("StronyStatyczne", "Home", new { nazwa = "Regulamin" })" style="">Regulamin</a>
                   

     


                <a id="koszykHeaderLink" href="@Url.Action("Index", "Koszyk")">
                    Koszyk (<span id="koszyk-naglowek-ilosc-elementow">@Html.Action("PobierzIloscElementowKoszyka", "Koszyk")</span>)
                    <img src="../Content/Images/koszyk_white.png" alt="Alternate Text" />
                    <span></span>
                </a>

                @Html.Partial("_LoginPartial")

        </header>


        <div class="container">
            @RenderBody()
        
            <div class="container">
                <footer>


                    <div>
                        Copyright &copy; 2017 Krzeszo
                    </div>
                </footer>
            </div>

                @RenderSection("Scripts", required: false)
            </div>

    
</body>
</html>